package mypsp.earcos.annotations;

import java.lang.annotation.ElementType;
import java.lang.annotation.Retention;
import java.lang.annotation.RetentionPolicy;
import java.lang.annotation.Target;

/**
 * 
 * @author Erik
 */
@Retention(RetentionPolicy.RUNTIME)
@Target(value = { ElementType.TYPE, ElementType.METHOD, ElementType.CONSTRUCTOR })
public @interface LogD {
	public static final String PLANNING = "planificacion";
	public static final String DESIGN = "diseno";
	public static final String CODING = "codificacion";
	public static final String TESTING = "pruebas";
	public static final String POSTMORTEM = "postmortem";
	public static final String INSPECTION = "inspeccion";

	String stage() default "";

	String type() default "";
}
