<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="jaxws-build.xml" id="jaxws"/>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>Mailer</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library>file.reference.dsn.jar</included-library>
            <included-library>file.reference.imap.jar</included-library>
            <included-library>file.reference.mail.jar</included-library>
            <included-library>file.reference.mailapi.jar</included-library>
            <included-library>file.reference.pop3.jar</included-library>
            <included-library>file.reference.smtp.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
    </configuration>
</project>
