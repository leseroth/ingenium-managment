<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://ws.pomanager.marketplace.losalpes.com.co/" xmlns:tns="http://ws.pomanager.marketplace.losalpes.com.co/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="AvisoDespachoNoExisteException" type="tns:AvisoDespachoNoExisteException"/>

  <xs:element name="ClienteNoExisteException" type="tns:ClienteNoExisteException"/>

  <xs:element name="OrdenCompraNoExisteException" type="tns:OrdenCompraNoExisteException"/>

  <xs:element name="consultarDA" type="tns:consultarDA"/>

  <xs:element name="consultarDAResponse" type="tns:consultarDAResponse"/>

  <xs:element name="consultarDAnumSeguimientoPO" type="tns:consultarDAnumSeguimientoPO"/>

  <xs:element name="consultarDAnumSeguimientoPOResponse" type="tns:consultarDAnumSeguimientoPOResponse"/>

  <xs:element name="consultarDAsFabricante" type="tns:consultarDAsFabricante"/>

  <xs:element name="consultarDAsFabricanteResponse" type="tns:consultarDAsFabricanteResponse"/>

  <xs:element name="consultarFabricantesConocidos" type="tns:consultarFabricantesConocidos"/>

  <xs:element name="consultarFabricantesConocidosResponse" type="tns:consultarFabricantesConocidosResponse"/>

