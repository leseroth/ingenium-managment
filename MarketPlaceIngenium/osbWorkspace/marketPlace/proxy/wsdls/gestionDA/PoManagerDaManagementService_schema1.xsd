  </xs:complexType>

  <xs:complexType name="consultarDAsFabricanteResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:dispatchAdviceBO" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ClienteNoExisteException">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
