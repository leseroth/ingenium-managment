  <xs:complexType name="consultarFabricantesConocidosResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:fabricanteBO" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="registrarDA">
    <xs:sequence>
      <xs:element name="da" type="tns:dispatchAdviceBO" minOccurs="0"/>
      <xs:element name="numSeguimientoPo" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="registrarDAResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OrdenCompraNoExisteException">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="consultarDAnumSeguimientoPO">
    <xs:sequence>
      <xs:element name="numSeguimiento" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="consultarDAnumSeguimientoPOResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:dispatchAdviceBO" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

