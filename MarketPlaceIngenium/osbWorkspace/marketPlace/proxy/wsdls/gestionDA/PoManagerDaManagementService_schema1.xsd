  <xs:element name="registrarDA" type="tns:registrarDA"/>

  <xs:element name="registrarDAResponse" type="tns:registrarDAResponse"/>

  <xs:complexType name="consultarDA">
    <xs:sequence>
      <xs:element name="numSeguimiento" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="consultarDAResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:dispatchAdviceBO" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="dispatchAdviceBO">
    <xs:sequence>
      <xs:element name="camiones" type="xs:string" minOccurs="0"/>
      <xs:element name="fabricante" type="tns:fabricanteBO" minOccurs="0"/>
