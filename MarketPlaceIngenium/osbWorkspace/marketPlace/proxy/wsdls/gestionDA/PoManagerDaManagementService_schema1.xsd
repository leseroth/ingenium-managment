  <xs:complexType name="comercioBO">
    <xs:sequence>
      <xs:element name="id" type="xs:long" minOccurs="0"/>
      <xs:element name="nit" type="xs:string" minOccurs="0"/>
      <xs:element name="nombre" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AvisoDespachoNoExisteException">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="consultarProductosFabricante">
    <xs:sequence>
      <xs:element name="nitFabricante" type="xs:string" minOccurs="0"/>
      <xs:element name="nitComercio" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="consultarProductosFabricanteResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:productoBO" minOccurs="0" maxOccurs="unbounded"/>
