<process name="RegistroEntidadInterNal"
         targetNamespace="http://xmlns.oracle.com/MarketPlace/ProcesoRegistroInterNal/RegistroEntidadInterNal"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:client="http://xmlns.oracle.com/MarketPlace/ProcesoRegistroInterNal/RegistroEntidadInterNal"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://marketplace.losalpes.com.co/GestionCliente"
         xmlns:ns2="http://marketplace.losalpes.com.co"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns3="http://marketplace.losalpes.com.co/GestionCorreoElectronico"
         xmlns:ns4="http://marketplace.losalpes.com.co/VerificacionCrediticia"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="registroentidadinternal_client"
                     partnerLinkType="client:RegistroEntidadInterNal"
                     myRole="RegistroEntidadInterNalProvider"
                     partnerRole="RegistroEntidadInterNalRequester"/>
        <partnerLink name="GestionCliente"
                     partnerLinkType="ns1:GestionCliente_PL"
                     partnerRole="GestionCliente_Role"/>
        <partnerLink name="GestionCorreoElectronico"
                     partnerLinkType="ns3:GestionCorreoElectronico_PL"
                     partnerRole="GestionCorreoElectronico_Role"/>
        <partnerLink name="VerificacionCrediticia"
                     partnerLinkType="ns4:VerificacionCrediticia_PL"
                     partnerRole="VerificacionCrediticia_Role"/>
    </partnerLinks>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable"
                  messageType="client:RegistroEntidadInterNalRequestMessage"/>
        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable"
                  messageType="client:RegistroEntidadInterNalResponseMessage"/>
        <variable name="ConsultarCliente_ConsultarCliente_InputVariable"
                  messageType="ns1:ConsultarClienteRequest"/>
        <variable name="ConsultarCliente_ConsultarCliente_OutputVariable"
                  messageType="ns1:ConsultarClienteResponse"/>
        <variable name="CrearCliente_CrearCliente_InputVariable"
                  messageType="ns1:CrearClienteRequest"/>
        <variable name="CrearCliente_CrearCliente_OutputVariable"
                  messageType="ns1:CrearClienteResponse"/>
        <variable name="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                  messageType="ns3:enviarCorreoElectronicoRequest"/>
        <variable name="EnviarCorreoFalloRegistro_enviarCorreoElectronico_OutputVariable"
                  messageType="ns3:enviarCorreoElectronicoResponse"/>
        <variable name="ActualizarCliente_ModificarCliente_InputVariable"
                  messageType="ns1:ModificarClienteRequest"/>
        <variable name="ActualizarCliente_ModificarCliente_OutputVariable"
                  messageType="ns1:ModificarClienteResponse"/>
        <variable name="ConsultarConfecamaras_VerificarComfecamaras_InputVariable"
                  messageType="ns4:VerificarComfecamarasRequest"/>
        <variable name="ConsultarConfecamaras_VerificarComfecamaras_OutputVariable"
                  messageType="ns4:VerificarComfecamarasResponse"/>
        <variable name="ConsultarDatacredito_VerificarDataCredito_InputVariable"
                  messageType="ns4:VerificarDataCreditoRequest"/>
        <variable name="ConsultarDatacredito_VerificarDataCredito_OutputVariable"
                  messageType="ns4:VerificarDataCreditoResponse"/>
        <variable name="CrearContacto_CrearContactos_InputVariable"
                  messageType="ns1:CrearContactosRequest"/>
        <variable name="CrearContacto_CrearContactos_OutputVariable"
                  messageType="ns1:CrearContactosResponse"/>
        <variable name="indiceContactos" type="xsd:int"/>
        <variable name="idCliente" type="xsd:string"/>
        <variable name="indiceProductos" type="xsd:int"/>
        <variable name="CrearProducto_CrearProductos_InputVariable"
                  messageType="ns1:CrearProductosRequest"/>
        <variable name="CrearProducto_CrearProductos_OutputVariable"
                  messageType="ns1:CrearProductosResponse"/>
        <variable name="ConsultarAntilavado_VerificarListaAntilavado_InputVariable"
                  messageType="ns4:VerificarListaAntilavadoRequest"/>
        <variable name="ConsultarAntilavado_VerificarListaAntilavado_OutputVariable"
                  messageType="ns4:VerificarListaAntilavadoResponse"/>
        <variable name="ConsultarListaClinton_VerificarListaClinton_InputVariable"
                  messageType="ns4:VerificarListaClintonRequest"/>
        <variable name="ConsultarListaClinton_VerificarListaClinton_OutputVariable"
                  messageType="ns4:VerificarListaClintonResponse"/>
        <variable name="ConsultarContacto_ConsultarContacto_InputVariable"
                  messageType="ns1:ConsultarContactoRequest"/>
        <variable name="ConsultarContacto_ConsultarContacto_OutputVariable"
                  messageType="ns1:ConsultarContactoResponse"/>
        <variable name="ActualizarContacto_ModificarContacto_InputVariable"
                  messageType="ns1:ModificarContactoRequest"/>
        <variable name="ActualizarContacto_ModificarContacto_OutputVariable"
                  messageType="ns1:ModificarContactoResponse"/>
    </variables>
    <faultHandlers>
        <catch faultName="bpelx:remoteFault">
            <sequence name="Sequence1">
                <assign name="AsignarCorreoElectronico">
                    <copy>
                        <from variable="inputVariable" part="payload"
                              query="/client:process/client:email"/>
                        <to variable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                            part="parameters"
                            query="/ns2:enviarCorreoElectronico/ns2:to"/>
                    </copy>
                    <copy>
                        <from expression="ora:format('ingenium.uniandes@gmail.com')"/>
                        <to variable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                            part="parameters"
                            query="/ns2:enviarCorreoElectronico/ns2:from"/>
                    </copy>
                    <copy>
                        <from expression="ora:format('ingenium2011')"/>
                        <to variable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                            part="parameters"
                            query="/ns2:enviarCorreoElectronico/ns2:password"/>
                    </copy>
                    <copy>
                        <from expression="ora:format('&lt;html>&lt;body>Marketplace de los Alpes informa:&lt;br/>&lt;br/>El proceso de registro de su entidad ha presentado un inconveniete.&lt;br/>Solicitamos por favor volver a intentar el proceso de registro.&lt;br/>Gracias.&lt;br/>Este es un mensaje autogenerado. Por favor no responda este mensaje.&lt;/body>&lt;/html>')"/>
                        <to variable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                            part="parameters"
                            query="/ns2:enviarCorreoElectronico/ns2:message"/>
                    </copy>
                    <copy>
                        <from expression="ora:format('PROCESO DE REGISTRO ')"/>
                        <to variable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                            part="parameters"
                            query="/ns2:enviarCorreoElectronico/ns2:subject"/>
                    </copy>
                </assign>
                <invoke name="EnviarCorreoFalloRegistro"
                        bpelx:invokeAsDetail="no"
                        inputVariable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_InputVariable"
                        outputVariable="EnviarCorreoFalloRegistro_enviarCorreoElectronico_OutputVariable"
                        partnerLink="GestionCorreoElectronico"
                        portType="ns3:GestionCorreoElectronico"
                        operation="enviarCorreoElectronico"/>
            </sequence>
        </catch>
    </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in RegistroEntidadInterNal.wsdl) -->
        <receive name="receiveInput"
                 partnerLink="registroentidadinternal_client"
                 portType="client:RegistroEntidadInterNal" operation="process"
                 variable="inputVariable" createInstance="yes"/>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="AsignarConsultarCliente">
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/client:process/client:nit"/>
                <to variable="ConsultarCliente_ConsultarCliente_InputVariable"
                    part="parameters" query="/ns2:consultarCliente/ns2:nit"/>
            </copy>
        </assign>
        <invoke name="ConsultarCliente"
                inputVariable="ConsultarCliente_ConsultarCliente_InputVariable"
                outputVariable="ConsultarCliente_ConsultarCliente_OutputVariable"
                partnerLink="GestionCliente" portType="ns1:GestionCliente"
                operation="ConsultarCliente" bpelx:invokeAsDetail="no"/>
        <switch name="Switch1">
            <case condition="string-length(bpws:getVariableData('ConsultarCliente_ConsultarCliente_OutputVariable','parameters','/ns2:consultarClienteResponse/ns2:cliente/nit')) = 0">
                <bpelx:annotation>
                    <bpelx:pattern>El cliente no existe en el CRM</bpelx:pattern>
                    <bpelx:general>
                        <bpelx:property name="userLabel">NoExiste</bpelx:property>
                    </bpelx:general>
                </bpelx:annotation>
                <sequence>
                    <assign name="AsignarCrearCliente">
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:nit"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/nit"/>
                        </copy>
                        <copy>
                            <from expression="1000"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/comision"/>
                        </copy>
                        <copy>
                            <from expression="'Inactivo'"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/estado"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:Categoria"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/categoria"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:tipo"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/tipo"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:razonSocial"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/razonSocial"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:nombre"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/nombre"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:direccion"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/direccion"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:telefono"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/telefono"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:email"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/email"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:CodPostal"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/codigoPostal"/>
                        </copy>
                        <copy>
                            <from variable="inputVariable" part="payload"
                                  query="/client:process/client:CodPais"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/codigoPais"/>
                        </copy>
                    </assign>
                    <invoke name="CrearCliente" bpelx:invokeAsDetail="no"
                            inputVariable="CrearCliente_CrearCliente_InputVariable"
                            outputVariable="CrearCliente_CrearCliente_OutputVariable"
                            partnerLink="GestionCliente"
                            portType="ns1:GestionCliente"
                            operation="CrearCliente"/>
                    <assign name="AsignarCrearContactos">
                        <copy>
                            <from variable="CrearCliente_CrearCliente_OutputVariable"
                                  part="parameters"
                                  query="/ns2:crearClienteResponse/ns2:idCuenta"/>
                            <to variable="CrearContacto_CrearContactos_InputVariable"
                                part="parameters"
                                query="/ns2:crearContacto/ns2:idCliente"/>
                        </copy>
                        <copy>
                            <from variable="CrearCliente_CrearCliente_OutputVariable"
                                  part="parameters"
                                  query="/ns2:crearClienteResponse/ns2:idCuenta"/>
                            <to variable="idCliente"/>
                        </copy>
                        <copy>
                            <from expression="1"/>
                            <to variable="indiceContactos"/>
                        </copy>
                    </assign>
                    <while name="WhileValorIndiceContactos"
                           condition="bpws:getVariableData('indiceContactos') &lt;= count(bpws:getVariableData('inputVariable','payload','/client:process/client:contactos'))">
                        <sequence name="Sequence3">
                            <assign name="AsignarDatosContacto">
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:numIdentificacion'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:numIdentificacion"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:tipoIdentificacion'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:tipoIdentificacion"/>
                                </copy>
                                <copy>
                                    <from variable="CrearCliente_CrearCliente_OutputVariable"
                                          part="parameters"
                                          query="/ns2:crearClienteResponse/ns2:idCuenta"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:idCliente"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:nombre'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:nombre"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:direccion'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:direccion"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:telefono'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:telefono"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:celular'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:celular"/>
                                </copy>
                                <copy>
                                    <from expression="'Activo'"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:estado"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:contactos[',bpws:getVariableData('indiceContactos'),']/client:email'))"/>
                                    <to variable="CrearContacto_CrearContactos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearContacto/ns2:contactos/ns2:email"/>
                                </copy>
                            </assign>
                            <invoke name="CrearContacto"
                                    inputVariable="CrearContacto_CrearContactos_InputVariable"
                                    outputVariable="CrearContacto_CrearContactos_OutputVariable"
                                    partnerLink="GestionCliente"
                                    portType="ns1:GestionCliente"
                                    operation="CrearContactos"
                                    bpelx:invokeAsDetail="no"/>
                            <assign name="IncrementarIndiceContactos">
                                <copy>
                                    <from expression="bpws:getVariableData('indiceContactos') + 1"/>
                                    <to variable="indiceContactos"/>
                                </copy>
                            </assign>
                        </sequence>
                    </while>
                    <assign name="AsignarCrearProductos">
                        <copy>
                            <from expression="1"/>
                            <to variable="indiceProductos"/>
                        </copy>
                        <copy>
                            <from variable="idCliente"/>
                            <to variable="CrearProducto_CrearProductos_InputVariable"
                                part="parameters"
                                query="/ns2:crearProducto/ns2:idCliente"/>
                        </copy>
                    </assign>
                    <while name="WhileValorIndiceProductos"
                           condition="bpws:getVariableData('indiceProductos') &lt;= count(bpws:getVariableData('inputVariable','payload','/client:process/client:productos'))">
                        <sequence name="Sequence4">
                            <assign name="AsignarDatosProducto">
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:productos[',bpws:getVariableData('indiceProductos'),']/client:nombre'))"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/nombre"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:productos[',bpws:getVariableData('indiceProductos'),']/client:tipo'))"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/tipo"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:productos[',bpws:getVariableData('indiceProductos'),']/client:pesoLibras'))"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/pesoLibras"/>
                                </copy>
                                <copy>
                                    <from expression="'Activo'"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/estado"/>
                                </copy>
                                <copy>
                                    <from variable="CrearCliente_CrearCliente_OutputVariable"
                                          part="parameters"
                                          query="/ns2:crearClienteResponse/ns2:idCuenta"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:idCliente"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:productos[',bpws:getVariableData('indiceProductos'),']/client:categoria'))"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/categoria"/>
                                </copy>
                                <copy>
                                    <from expression="bpws:getVariableData('inputVariable','payload',concat('/client:process/client:productos[',bpws:getVariableData('indiceProductos'),']/client:referencia'))"/>
                                    <to variable="CrearProducto_CrearProductos_InputVariable"
                                        part="parameters"
                                        query="/ns2:crearProducto/ns2:productos/referencia"/>
                                </copy>
                            </assign>
                            <invoke name="CrearProducto"
                                    bpelx:invokeAsDetail="no"
                                    inputVariable="CrearProducto_CrearProductos_InputVariable"
                                    outputVariable="CrearProducto_CrearProductos_OutputVariable"
                                    partnerLink="GestionCliente"
                                    portType="ns1:GestionCliente"
                                    operation="CrearProductos"/>
                            <assign name="IncrementarIndiceProductos">
                                <copy>
                                    <from expression="bpws:getVariableData('indiceProductos') +1"/>
                                    <to variable="indiceProductos"/>
                                </copy>
                            </assign>
                        </sequence>
                    </while>
                    <assign name="AsignarEstadoInactivo">
                        <copy>
                            <from variable="CrearCliente_CrearCliente_OutputVariable"
                                  part="parameters"
                                  query="/ns2:crearClienteResponse/ns2:idCuenta"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/id"/>
                        </copy>
                        <copy>
                            <from expression="'Inactive'"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/estado"/>
                        </copy>
                        <copy>
                            <from expression="'1000'"/>
                            <to variable="CrearCliente_CrearCliente_InputVariable"
                                part="parameters"
                                query="/ns2:crearCliente/ns2:cliente/comision"/>
                        </copy>
                    </assign>
                </sequence>
            </case>
            <otherwise>
                <sequence>
                    <switch name="Switch3">
                        <case condition="bpws:getVariableData('ConsultarCliente_ConsultarCliente_OutputVariable','parameters','/ns2:consultarClienteResponse/ns2:cliente/estado') = 'Inactive'">
                            <sequence>
                                <assign name="AsignarActualizarCliente">
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/client:process/client:nombre"/>
                                        <to variable="ActualizarCliente_ModificarCliente_InputVariable"
                                            part="parameters"
                                            query="/ns2:modificarCliente/ns2:cliente/nombre"/>
                                    </copy>
                                    <copy>
                                        <from expression="'Inactivo'"/>
                                        <to variable="ActualizarCliente_ModificarCliente_InputVariable"
                                            part="parameters"
                                            query="/ns2:modificarCliente/ns2:cliente/estado"/>
                                    </copy>
