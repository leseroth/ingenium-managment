<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="TransactAuctionManagement">
      <implementation-class>co.com.losalpes.marketplace.transact.ws.TransactAuctionManagement</implementation-class>
    </service>
  </services>
  <clients>
    <client name="fedex">
      <wsdl-url>http://service.ecocoma.com/shipping/fedex.asmx?WSDL</wsdl-url>
      <local-wsdl-file>service.ecocoma.com/shipping/fedex.asmx.wsdl</local-wsdl-file>
      <package-name>com.ecocoma.service.shipping.fedex</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://service.ecocoma.com/shipping/fedex.asmx?WSDL</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
