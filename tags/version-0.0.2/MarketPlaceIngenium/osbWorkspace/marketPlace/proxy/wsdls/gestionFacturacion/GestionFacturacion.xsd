<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://marketplace.losalpes.com.co" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://marketplace.losalpes.com.co">

    <include schemaLocation="../../../canonicos/Cliente.xsd"></include>
    <include schemaLocation="../../../canonicos/CuentaFacturacion.xsd"></include>
    <complexType name="CrearCuentaFacturacion">
    	<sequence>
    		<element name="cliente" type="tns:Cliente" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="CrearCuentaFacturacionResponse">
    	<sequence>
    		<element name="numCuenta" type="string" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="ConsultarCuentaFacturacion">
    	<sequence>
    		<element name="numeroCuenta" type="string" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="ConsultarCuentaFacturacionResponse">
    	<sequence>
    		<element name="cuentaFacturacion" type="tns:CuentaFacturacion" minOccurs="0" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="GenerarFacturaCorte">
