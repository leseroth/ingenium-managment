    		<element name="corte" type="int" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="ReportarClientesMorososResponse">
    	<sequence>
    		<element name="resultado" type="boolean" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
