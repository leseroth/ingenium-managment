    </element>
    
    <element name="generarFacturaCorteResponse"
    	type="tns:GenerarFacturaCorteResponse">
    </element>

    <element name="registrarTransaccion"
    	type="tns:RegistrarTransaccion">
    </element>

    <element name="registrarTransaccionResponse"
    	type="tns:RegistrarTransaccionResponse">
    </element>

    <element name="reportarClientesMorosos" type="tns:ReportarClientesMorosos"></element>

