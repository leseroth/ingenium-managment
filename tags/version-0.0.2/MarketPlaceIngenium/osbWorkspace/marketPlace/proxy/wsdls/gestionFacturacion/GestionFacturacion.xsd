    	</sequence>
    </complexType>
    
    <complexType name="RegistrarTransaccionResponse">
    	<sequence>
    		<element name="resultado" type="boolean" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="CambiarEstadoCliente">
