    <plnk:partnerLinkType name="ConfirmacionPago">
        <plnk:role name="ConfirmacionPagoProvider">
            <plnk:portType name="client:ConfirmacionPago"/>
        </plnk:role>
        <plnk:role name="ConfirmacionPagoRequester">
            <plnk:portType name="client:ConfirmacionPagoCallback"/>
        </plnk:role>
